{
  "configurations": [
    {
      "name": "tal_dev",
      "intelliSenseMode": "gcc-arm",
      "cStandard": "c11",
      "compilerArgs": [
        "-mcpu=cortex-m7",
        "-ffreestanding",
        "-nostdlib",
        "-Wpedantic",
        "-Wall",
        "-Wextra"
      ],
      "includePath": [
        "${workspaceFolder}"
      ],
      "compilerPath": "${env:ARM_GNU_TOOLCHAIN}/bin/arm-none-eabi-gcc",
      "defines": [
        "TAL_DEBUG",
        "TAL_TARGET_GCC_ARM_STM32H745_CM7",
        "TAL_PORT_DIR = gcc_arm_stm32h745_cm7"
      ]
    }
  ],
  "version": 4
}