{
    "version":"0.2.0",
    "configurations": [
        {
            "name": "Cortex ST-Link",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceFolder}/build/TAL.elf",
            "gdbPath": "${env:ARM_GNU_TOOLCHAIN}/bin/arm-none-eabi-gdb",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "stutil",
            "stlinkPath": "${env:ST_LINK}/st-util.exe",
            "armToolchainPath": "${env:ARM_GNU_TOOLCHAIN}/bin",
            "stm32cubeprogrammer": "${env:ST_CUBE_PROGRAMMER}/bin",
            "device": "STM32H745_CM7",
            "svdFile": "${workspaceFolder}/dev/devboard/STM32H745_CM7.svd",
        },  
        {
            "name": "Cortex Debug",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceFolder}/build/TAL.elf",
            "gdbPath": "${env:ARM_GNU_TOOLCHAIN}/bin/arm-none-eabi-gdb",
            "armToolchainPath": "${env:ARM_GNU_TOOLCHAIN}/bin",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "device": "STM32H745_CM7",
            "configFiles": [
              "${workspaceFolder}/dev/devboard/flash_config.cfg",
            ],
            "svdFile": "${workspaceFolder}/dev/devboard/STM32H745_CM7.svd",
        }       
    ]
}