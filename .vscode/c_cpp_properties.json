{
  "configurations": [
    {
      "name": "tal_dev",
      "intelliSenseMode": "gcc-arm",
      "cStandard": "gnu17",
      "compilerArgs": [
        "-mcpu=cortex-m7",
        "-ffreestanding",
        "-nostdlib",
        "-Wpedantic",
        "-Wall",
        "-Wextra"
      ],
      "includePath": [
        "${workspaceFolder}"
      ],
      "compilerPath": "${env:ARM_GNU_TOOLCHAIN}/bin/arm-none-eabi-gcc",
      "defines": [
        "TI_DEBUG",
        "TI_SEMIHOSTING",
        "TI_TARGET_STM32H7x5"
      ]
    }
  ],
  "version": 4
}